author = {Cornish, E A and Fisher, R A},
issn = {03731138},
journal = {Revue de l'Institut International de Statistique / Review of the International Statistical Institute},
keywords = {TCC},
mendeley-tags = {TCC},
number = {4},
pages = {307--320},
publisher = {[International Statistical Institute (ISI), Wiley]},
title = {{Moments and Cumulants in the Specification of Distributions}},
url = {http://www.jstor.org/stable/1400905},
volume = {5},
year = {1938}
}
